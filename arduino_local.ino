const char *ssid = "<nome da rede>";
const char *password = "<senha da rede>";

/// Configuracao do NTP cliente ///
WiFiUDP ntpUDP;
NTPClient timeClient(ntpUDP, "pool.ntp.org", -3 * 60 * 60); 

void setup() {
  SPI.begin();
  Serial.begin(9600);
  mfrc522.PCD_Init();
  Serial.println("RFID lendo UID");

  /// Conexao com a rede wifi ///
  WiFi.begin(ssid, password);
  while (WiFi.status() != WL_CONNECTED) {
    delay(1000);
    Serial.println("Conectando ao WiFi...");
  }
  Serial.println("Conectado!!!");

  /// inicia o servidor NTP ///
  timeClient.begin();
}

void loop() {
  /// verificacao a respeito de uma nova tag ///
  if (mfrc522.PICC_IsNewCardPresent()) {
    if (mfrc522.PICC_ReadCardSerial()) {

      String tagUID = getTagUID();
      Permission permission = checkPermission(tagUID);

      timeClient.update(); 

      Serial.print("Horario atual: ");
      Serial.print(timeClient.getHours());
      Serial.print(":");
      if (timeClient.getMinutes() < 10) {
        Serial.print("0");
      }
      Serial.println(timeClient.getMinutes());
      Serial.print("A pessoa que possue o cartao eh: ");
      Serial.println(permission.name);
      Serial.print("Permissao obtida - TAG: ");
      Serial.println(tagUID);
      Serial.print("Permissao: ");
      Serial.print(permission.permission);
      Serial.print(" | Inicio: ");
      Serial.print(permission.startHour);
      Serial.print(" | Fim: ");
      Serial.println(permission.endHour);
      

      if (isTimeValid(permission, timeClient.getHours())) {
        Serial.print("Acesso Permitido - TAG: ");
        Serial.println(tagUID);
        
      } else {
        Serial.print("Acesso Negado - Horario nao permitido ou cartao nao cadastrado - TAG: ");
        Serial.println(tagUID);
        
      }

      mfrc522.PICC_HaltA();
    }
  }
}

/// Funcao para obter o UID do cartao como uma string ///
String getTagUID() {
  String tagUID = "";
  for (byte i = 0; i < mfrc522.uid.size; i++) {
    tagUID += (mfrc522.uid.uidByte[i] < 0x10 ? "0" : "");
    tagUID += String(mfrc522.uid.uidByte[i], HEX);
  }
  return tagUID;
}

/// Funcao para verificar a permissao ///
Permission checkPermission(String tagUID) {
  tagUID.toLowerCase();  
  for (int i = 0; i < 500; i++) { /// Parte do codigo que diz ate onde serao considerados os registros
    if (tagUID.equals(permissions[i].uid)) {
      return permissions[i];
    }
  }

  return {"", "DENIED", 0, 0};
}

/// Verifica se a hora atual esta conforme a permissao ///
bool isTimeValid(Permission permission, int currentHour) {
  if (permission.startHour <= permission.endHour) {
    return (currentHour >= permission.startHour && currentHour <= permission.endHour);
  } else {
    
    return (currentHour >= permission.startHour || currentHour <= permission.endHour);
  }
}
